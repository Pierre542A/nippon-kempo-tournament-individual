{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api-fastify/index.js": {
      "runtime": "nodejs18.x",
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api-fastify/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ]
}