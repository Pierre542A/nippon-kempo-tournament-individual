{
  "buildCommand": "cd front-quasar && npm install && npm run build",
  "outputDirectory": "front-quasar/dist/spa",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api-fastify"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ],
  "functions": {
    "api-fastify/index.js": {
      "maxDuration": 10
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}